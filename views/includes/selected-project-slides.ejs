<article class="cards" id="slides">
  
  <div class="imagens-do-projeto">
    
    <div class="image-selected">
      <button id="btn-anterior"><i class="fas fa-chevron-left"></i></button>
      <img src="/img/upload/<%=project.images[0]%>" alt="imagem" data-index="0" id="slide-atual">
      <button id="btn-proximo"><i class="fas fa-chevron-right"></i></button>
    </div>
    
    <% for( var i = 1; i < project.images.length; i++ ) { %>
      <div class="fila-de-imagem">
        <img src="/img/upload/<%=project.images[i]%>" alt="imagem" data-index="<%=i%>">
      </div>
    <% } %>
      
  </div>
  
</article>